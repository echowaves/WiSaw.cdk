{
  "name": "wi_saw.cdk",
     "version": "0.1.0", 
  "bin": {
    "wi_saw.cdk": "bin/wi_saw.cdk.js"
  },
  "scripts": {
    "test": "./node_modules/nyc/bin/nyc.js --reporter=html ./node_modules/mocha/bin/mocha --exit --require @babel/register ./tests/*.js --timeout 30000",
    "test:deploy": "DEPLOY_ENV=test npx cdk deploy",
    "test:destroy": "DEPLOY_ENV=test npx cdk destroy test-WiSawCdkStack",
    "prod:deploy": "DEPLOY_ENV=prod npx cdk deploy",
    "prod:destroy": "DEPLOY_ENV=prod npx cdk destroy prod-WiSawCdkStack",
    "migrate": "./node_modules/.bin/sequelize db:migrate --env",
    "migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo --env",
    "populate-dimensions": "node ./scripts/populate-photo-dimensions.js",
    "populate-dimensions:dry-run": "node ./scripts/populate-photo-dimensions.js --dry-run",
    "populate-recognitions": "node ./scripts/populate-recognitions.js",
    "populate-recognitions:dry-run": "node ./scripts/populate-recognitions.js --dry-run",
    "cleanup-s3": "node ./scripts/cleanup-s3.js",
    "cleanup-s3:dry-run": "node ./scripts/cleanup-s3.js --dry-run",
    "lint": "npx ts-standard",
    "lint:fix": "npx ts-standard --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/register": "^7.28.3",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "aws-cdk": "^2.1030.0",
    "chai": "^6.2.0",
    "chai-http": "5.1.2",
    "dotenv": "^17.2.3",
    "esbuild": "^0.25.4",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.6.0",
    "lambda-log": "3.1.0",
    "mocha": "^11.7.4",
    "nyc": "17.1.0",
    "pg": "^8.16.3",
    "regenerator-runtime": "0.14.1",
    "sequelize": "6.37.7",
    "sequelize-cli": "6.6.3",
    "supertest": "^7.1.4",
    "ts-node": "10.9.2",
    "ts-standard": "^12.0.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.806.0",
    "@aws-sdk/client-rekognition": "^3.806.0",
    "@aws-sdk/client-s3": "^3.806.0",
    "@aws-sdk/client-sso-oidc": "^3.806.0",
    "@aws-sdk/client-sts": "^3.806.0",
    "@aws-sdk/s3-request-presigner": "^3.806.0",
    "aws-cdk-lib": "^2.220.0",
    "class-transformer": "0.5.1",
    "constructs": "^10.3.0",
    "global": "4.4.0",
    "moment": "2.30.1",
    "node-fetch": "^3.3.2",
    "serverless-postgres": "^2.1.2",
    "sharp": "^0.34.1",
    "sitemap": "8.0.0",
    "source-map-support": "0.5.21",
    "uuid": "^13.0.0"
  }
}
