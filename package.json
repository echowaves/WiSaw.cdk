{
  "name": "wi_saw.cdk",
  "version": "0.1.0",
  "bin": {
    "wi_saw.cdk": "bin/wi_saw.cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "./node_modules/nyc/bin/nyc.js --reporter=html ./node_modules/mocha/bin/mocha --exit --require @babel/register ./tests/*.js --timeout 30000",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "build-lambdas": "pushd lambda-fns; rm lambdas.zip; yarn build; zip -q -r lambdas.zip *; popd",
    "test:deploy": "DEPLOY_ENV=test yarn build && yarn build-lambdas && DEPLOY_ENV=test npx cdk deploy -O cdk-exports.json",
    "test:destroy": "DEPLOY_ENV=test cdk destroy test-WiSawCdkStack",
    "prod:deploy": "DEPLOY_ENV=prod yarn build && yarn build-lambdas && DEPLOY_ENV=prod npx cdk deploy -O cdk-exports.json",
    "prod:destroy": "DEPLOY_ENV=prod cdk destroy prod-WiSawCdkStack",
    "migrate": "./node_modules/.bin/sequelize db:migrate --env",
    "migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo --env"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "@types/node": "18.11.10",
    "aws-cdk-lib": "^2.80.0",
    "babel-eslint": "10.1.0",
    "chai": "4.3.7",
    "chai-http": "4.3.0",
    "constructs": "^10.2.31",
    "dotenv": "16.0.3",
    "eslint": "8.28.0",
    "eslint-plugin-import": "2.26.0",
    "lambda-log": "3.1.0",
    "mocha": "10.1.0",
    "nyc": "15.1.0",
    "pg": "8.11.0",
    "regenerator-runtime": "0.13.11",
    "sequelize": "6.31.1",
    "sequelize-cli": "6.6.0",
    "supertest": "6.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.80.0",
    "constructs": "^10.2.31",
    "global": "4.4.0",
    "moment": "2.29.4",
    "node-fetch": "^3.3.0",
    "serverless-postgres": "2.0.2",
    "source-map-support": "0.5.21",
    "uuid": "9.0.0"
  }
}
