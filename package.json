{
  "name": "wi_saw.cdk",
  "version": "0.1.0",
  "bin": {
    "wi_saw.cdk": "bin/wi_saw.cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "test:deploy": "DEPLOY_ENV=test yarn build && DEPLOY_ENV=test npx cdk deploy -O cdk-exports.json",
    "test:destroy": "DEPLOY_ENV=test cdk destroy test-WiSawCdkStack",
    "prod:deploy": "DEPLOY_ENV=prod yarn build && DEPLOY_ENV=prod npx cdk deploy -O cdk-exports.json",
    "migrate": "./node_modules/.bin/sequelize db:migrate --env",
    "migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo --env"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.98.0",
    "@types/jest": "^26.0.10",
    "@types/node": "14.14.37",
    "aws-cdk": "1.98.0",
    "jest": "^26.4.2",
    "pg": "8.6.0",
    "pg-hstore": "2.3.3",
    "sequelize": "6.6.2",
    "sequelize-cli": "6.2.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.4"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "^1.73.0",
    "@aws-cdk/aws-ec2": "^1.73.0",
    "@aws-cdk/aws-lambda": "^1.73.0",
    "@aws-cdk/aws-rds": "^1.73.0",
    "@aws-cdk/core": "1.98.0",
    "dotenv": "8.2.0",
    "source-map-support": "^0.5.16"
  }
}
