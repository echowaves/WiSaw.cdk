{
  "name": "wi_saw.cdk",
  "version": "0.1.0",
  "bin": {
    "wi_saw.cdk": "bin/wi_saw.cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "./node_modules/nyc/bin/nyc.js --reporter=html ./node_modules/mocha/bin/mocha --exit --require @babel/register ./tests/*.js --timeout 30000",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "build-lambdas": "pushd lambda-fns; rm lambdas.zip; yarn build; zip -q -r lambdas.zip *; popd",
    "test:deploy": "DEPLOY_ENV=test yarn build && yarn build-lambdas && DEPLOY_ENV=test npx cdk deploy -O cdk-exports.json",
    "test:destroy": "DEPLOY_ENV=test cdk destroy test-WiSawCdkStack",
    "prod:deploy": "DEPLOY_ENV=prod yarn build && yarn build-lambdas && DEPLOY_ENV=prod npx cdk deploy -O cdk-exports.json",
    "prod:destroy": "DEPLOY_ENV=prod cdk destroy prod-WiSawCdkStack",
    "migrate": "./node_modules/.bin/sequelize db:migrate --env",
    "migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo --env"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.24.1",
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.12",
    "@babel/preset-env": "7.17.12",
    "@babel/register": "7.17.7",
    "@types/node": "17.0.34",
    "aws-cdk": "2.24.1",
    "babel-eslint": "10.1.0",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "dotenv": "16.0.1",
    "eslint": "8.15.0",
    "eslint-plugin-import": "2.26.0",
    "lambda-log": "3.1.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "pg": "8.7.3",
    "regenerator-runtime": "0.13.9",
    "sequelize": "6.19.2",
    "sequelize-cli": "6.4.1",
    "supertest": "6.2.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "1.156.1",
    "@aws-cdk/aws-ec2": "1.156.1",
    "@aws-cdk/aws-events-targets": "1.156.1",
    "@aws-cdk/aws-iam": "1.156.1",
    "@aws-cdk/aws-lambda": "1.156.1",
    "@aws-cdk/aws-rds": "1.156.1",
    "@aws-cdk/aws-s3-notifications": "1.156.1",
    "@aws-cdk/aws-secretsmanager": "1.156.1",
    "@aws-cdk/core": "1.156.1",
    "global": "4.4.0",
    "moment": "2.29.3",
    "serverless-postgres": "1.10.1",
    "source-map-support": "0.5.21",
    "uuid": "8.3.2"
  }
}
