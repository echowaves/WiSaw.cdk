{
  "name": "wi_saw.cdk",
  "version": "0.1.0",
  "bin": {
    "wi_saw.cdk": "bin/wi_saw.cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "./node_modules/nyc/bin/nyc.js --reporter=html ./node_modules/mocha/bin/mocha --exit --require @babel/register ./tests/*.js --timeout 30000",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "test:deploy": "DEPLOY_ENV=test yarn build && DEPLOY_ENV=test npx cdk deploy -O cdk-exports.json",
    "test:destroy": "DEPLOY_ENV=test cdk destroy test-WiSawCdkStack",
    "prod:deploy": "DEPLOY_ENV=prod yarn build && DEPLOY_ENV=prod npx cdk deploy -O cdk-exports.json",
    "migrate": "./node_modules/.bin/sequelize db:migrate --env",
    "migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo --env"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.100.0",
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.13.16",
    "@types/node": "14.14.41",
    "aws-cdk": "1.100.0",
    "babel-eslint": "^10.1.0",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "eslint": "7.25.0",
    "eslint-plugin-import": "2.22.1",
    "lambda-log": "3.0.0",
    "mocha": "8.3.2",
    "moment": "2.29.1",
    "nyc": "15.1.0",
    "pg": "8.6.0",
    "pg-hstore": "2.3.3",
    "sequelize": "6.6.2",
    "sequelize-cli": "6.2.0",
    "supertest": "6.1.3",
    "ts-node": "^9.1.1",
    "regenerator-runtime": "0.13.7",
    "typescript": "~4.2.4"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "^1.100.0",
    "@aws-cdk/aws-ec2": "^1.100.0",
    "@aws-cdk/aws-lambda": "^1.100.0",
    "@aws-cdk/aws-rds": "^1.100.0",
    "@aws-cdk/core": "1.100.0",
    "dotenv": "8.2.0",
    "source-map-support": "^0.5.16",
    "uuid": "8.3.2"
  }
}
