{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCloudFrontOAIAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity [OAI_ID]"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::wisaw.com/*"
    }
  ]
}
